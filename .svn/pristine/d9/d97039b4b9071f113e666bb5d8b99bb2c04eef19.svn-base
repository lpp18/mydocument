.recheckbox{
    position: relative;}
.recheckbox input{
    width:.6*1.875rem;
    height:.6*1.875rem;
    position: relative;
    top:-.2*1.875rem;
    outline:none;
    left:-.1*1.875rem;
    border:none;
    background: #fff;
    cursor: pointer;
    opacity: 0;
}
.recheckbox input:checked+span{
    opacity: 1;
}
.recheckbox label {
    cursor: pointer;
    height: 25px;
}
.recheckbox .lbord{
    position: relative;
}
.recheckbox .lbord span{
    opacity: 0;
    position: absolute;
    width: .2*1.875rem;
    height: .1*1.875rem;
    background: transparent;
    top: .08*1.875rem;
    left: .06*1.875rem;
    border: 3px solid #333;
    border-top: none;
    border-right: none;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
.recheckbox label.ochange:after {
    opacity: 1;
}
.lbord{width:.4*1.875rem;
       height:.4*1.875rem;
      border:1.5px solid #555;
     margin-right:5px;
     float:left;
     border-radius:.1*1.875rem;
     display:inline-block;
}
