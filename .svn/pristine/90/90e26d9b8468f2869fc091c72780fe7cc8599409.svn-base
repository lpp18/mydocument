@{
    ViewBag.Title = "方案决策";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<li>
    <h2 class="mydocument_infor_title">
        是否上会：
    </h2>
    <p id="labSfsh" class="mydocument_infor_title_text"></p>
</li>
<li>
    <h2 class="mydocument_infor_title">
        项目及采购需求简述：
    </h2>
    <p id="labdescription" class="mydocument_infor_title_text"></p>
</li>
<li>
    <h2 class="mydocument_infor_title">
        预（估）算金额：
    </h2>
    <p id="labprices" class="mydocument_infor_title_text"></p>
</li>
<li>
    <h2 class="mydocument_infor_title">
        建议采购方式：
    </h2>
    <p id="labadvice" class="mydocument_infor_title_text"></p>
</li>
<li>
    <h2 class="mydocument_infor_title">
