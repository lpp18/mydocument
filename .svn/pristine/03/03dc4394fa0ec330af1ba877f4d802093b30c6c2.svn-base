html,body,menu,ul,ol,li,dl,dt,dd,p,div,form,h1,h2,h3,h4,h5,h6,img,a img,input,textarea,fieldset,figure,figcaption {
    padding:0;
    margin:0;
    border:0;
}
ul,ol,li {
    list-style:none;
}
h1,h2,h3,h4,h5,h6,b,i,em {
    font-size:1rem;
    font-weight:normal;
    font-style:normal;
}
header,nav,section,footer {
    display:block;
    padding:0;
    margin:0;
    border:0;
}
table,tr,td {
    margin:0;
    padding:0;
    border-collapse:collapse;
}
img,input,select {
    vertical-align:middle;
}
body,input,textarea,select {
    -webkit-text-size-adjust:none;
    font:normal 16px/1.5 "\5FAE\8F6F\96C5\9ED1",Helvetica,verdana,san-serif;
}
input[type="text"],input[type="password"],input[type="button"],input[type="submit"],textarea {
    -webkit-appearance:none;
}
input::-webkit-input-placeholder {
    color:#EEEEEE !important;
}
input:-moz-placeholder {
    color:#EEEEEE !important;
}
input::-moz-placeholder {
    color:#EEEEEE !important;
}
input:-ms-input-placeholder {
    color:#EEEEEE !important;
}
input,button,select,textarea {
    outline:none;
}
textarea {
    resize:none;
}
li {
    list-style:none;
}
a {
    text-decoration:none;
}
i,em {
    font-style:normal;
}
.clearFix:after {
    content:'';
    clear:both;
    display:block;
}
.clearFix {
    zoom:1;
}
.fl {
    float:left;
}
.fr {
    float:right;
}
img {
    display:block;
}
.hidden {
    display:none;
}
.show {
    display:block;
}
/* element */
header {
    position: fixed;
    top:0;
    left:0;
    width: 100%;
    height: 2.25rem;
    box-sizing: border-box;
    padding: .8rem .75rem 0;
    background: #fff;
    z-index:10;
}
header .search {
    width: 100%;
    height: 100%;
    background-color: #f2f2f2;
    border-radius: .7rem;
    box-sizing: border-box;
    padding: 0 .7rem;
}
header .search_icon {
    display: inline-block;
    margin-top: .425rem;
    margin-right: .15rem;
    width: .6rem;
    height: .6rem;
    background: url("../img/Magnifier.png") no-repeat;
    background-size: 100% 100%;
}
header .search_box {
    display: inline-block;
    width: 12.5rem;
    height: 1rem;
    background: #f2f2f2;
    margin-top: .2125rem;
    font-size: .6rem;
}
.search_box::-webkit-input-placeholder { /* WebKit browsers */
    color: #999 !important;
}
.search_box:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #999 !important;
}
.search_box::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #999 !important;
}
.search_box:-ms-input-placeholder { /* Internet Explorer 10+ */
    color: #999 !important;
}
nav {
    position:fixed;
    top:2.25rem;
    width: 100%;
    height: 2.2rem;
    line-height: 2.2rem;
    border-bottom: 1px solid #cfccc7;
    background: #fff;
    z-index:10;
}
nav li {
    width: 33.333333%;
    height:2.2rem;
    text-align: center;
    font-size: .8rem;
    float: left;
    color: #999;
    box-sizing: border-box;
}
.nav_active {
    color: #353535;
    font-weight: 600;
    border-bottom: 2px solid #2fc3c3;
}
.electron_flow {
    width: 100%;
    height:auto;
}
#container li {
    position: relative;
    box-sizing: border-box;
    padding: 0 .7rem;
    border-bottom: 1px solid #cfccc7;
}
#container .my_title {
    font-size: .75rem;
    color: #353535;
    padding-top: .85rem;
}
#container .my_content {
    font-size: .65rem;
    color: #6c6c6c;
    padding-top: .6rem;
    width: 15rem;
    overflow:hidden;
    text-overflow:ellipsis; 
    white-space: nowrap;/*加宽度width属来兼容部分浏览*/
}
#container .my_time {
    font-size: .5rem;
    color: #888888;
    padding-top: .65rem;
    padding-bottom: .85rem;
}
.my_arrow {
    position: absolute;
    display: inline-block;
    width: .5rem;
    height: .8rem;
    top:45%;
    right:.7rem;
    background: url("../img/arrow.png") no-repeat;
    background-size: 100% 100%;
}