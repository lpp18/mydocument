#mydocument_nav_db {
    top:0;
}
#mydocument_nav_db li{
    width: 25%;
}
#mydocument_nav_dy {
    top:0;
}
#mydocument_nav_dy li{
    width: 33.33333333%;
}
.mydocument_infor li{
    display:flex;
    align-items:center;
    box-sizing: border-box;
    border-bottom: 1px solid #cfccc7;
    padding:.75rem .7rem;
    font-size: .65rem;
}
.mydocument_infor li:last-child{
     border-bottom: none;
}
.mydocument_infor li>h2 {
    display: inline-block;
    width: 27%;
    height:100%;
    text-align: left;
    font-size: .65rem;
    color: #666;
}
.mydocument_infor li>p {
	    font-size: .7rem;
    text-align: left;
    display: inline-block;
    width: 70%;
    height: 100%;
    color: #353535;
}
.mydocument_infor_deal {
    display: flex;
    flex: 1;
    width: 100%;
    height: auto;
    flex-direction: column;
}
.mydocument_infor_btn {
    display:none;
    position: fixed;
    bottom: 0;
    box-sizing: border-box;
    padding:0 2rem;
    width:100%;
    height:2.2rem;
    background: #2dc5c1;
    font-size: .75rem;
    color: #fff;
    line-height: 2.2rem;
    text-align: center;
    z-index: 1000;
}
.mydocument_infor_btn i {
    position:absolute;
    top:50%;
    margin-top:-.4rem;
    left: 50%;
    margin-left: -3rem;
    display: inline-block;
    width: .875rem;
    height:.8rem;
    background: url("../img/edit.png") no-repeat;
    background-size: 100% 100%;
}
.mydocument_infor_deal_type h3 {
    box-sizing: border-box;
    width: 100%;
    height:1.75rem;
    line-height: 1.75rem;
    background-color: #e8e8e8;
    color: #666;
    font-size: .6rem;
    padding: 0 .7rem;
    text-align: left;
}
.mydocument_infor_deal_type p {
	color:#555;
    box-sizing: border-box;
    padding:0 .7rem;
    width: 100%;
    height: 2rem;
    line-height: 2rem;
    font-size: .6rem;
    border-bottom: 1px solid #cfccc7 ;
}
.mydocument_infor_deal_type p input {
    margin-top: .7rem;
}
.mydocument_infor_deal_type p .select{
    position: relative;
    display: block;
    color: #999;
    margin-right: 1rem;
}
.mydocument_infor_deal_type p .select i {
    position: absolute;
    top:.6rem;
    right:-1rem;
    display: inline-block;
    width: .5rem;
    height:.8rem;
    background: url("../img/arrow.png") no-repeat;
    background-size: 100% 100%;
}
.mydocument_infor_deal h3 {
    box-sizing: border-box;
    height:1.75rem;
    line-height: 1.75rem;
    background-color: #e8e8e8;
    color: #353535;
    font-size: .6rem;
    padding: 0 .7rem;
    text-align:left;
}
.mydocument_infor_deal h4 {
    box-sizing: border-box;
    width: 100%;
    height:2rem;
    line-height: 2rem;
    color: #353535;
    font-size: .8rem;
    padding: 0 .7rem;
}
.mydocument_infor_deal .deal-content {
    flex:1;
    box-sizing: border-box;
    width: 100%;
    padding: .7rem;
}
.mydocument_infor_deal textarea {
    font-size:.75rem;
    display: block;
    overflow-x: hidden;
    overflow-y: hidden;
    box-sizing: border-box;
    padding:.6rem .5rem;
    width: 100%;
    height: 7.5rem;
    border:1px solid #aaa;
    border-radius: .5rem;
    text-align: left;
}
.has_see {
    margin-top:1.5rem;
    width: 100%;
    height: 2.5rem;
}
.has_see img {
    width: .875rem;
    height: .75rem;
  
}
.has_see p {
	color:#555;
    text-align: center;
    font-size: .7rem;
    color:#2ec4c2;
    margin-left:.2rem
}
.mydocument_infor_footer {
    position: relative;
    bottom: 0;
    /* width: 100%; */
    height: 2.2rem;
    border-top: 1px solid #cfcfcf;
    background: #fff;
}
.mydocument_infor_footer>p {
	color:#555;
    position: relative;
    width: 50%;
    height:2.2rem;
    line-height: 2.2rem;
    float: left;
    text-align: center;
    font-size: .75rem;
}
.mydocument_infor_footer>p i {
    position: absolute;
    display: inline-block;
    width: .8rem;
    height:.75rem;
    top:50%;
    margin-top: -.375rem;
    left:2.7rem;
}
.aggree {
    color: #2bc5c1;
}
.disagree {
    color: #e75050;
}
.agree_img {
    background: url("../img/aggree.png") no-repeat;
    background-size: 100% 100%;
}
.disagree_img {
    background: url("../img/reject.png") no-repeat;
    background-size: 100% 100%;
}

.footer_line {
    width: 1px;
    border-left:2px solid #2bc5c1;
    height:.8rem;
    position: absolute;
    left: 50%;
    margin-left:-1px;
    top:50%;
    margin-top: -.4rem;
}