.checbox {
  width: 0.61875rem;
  height: 0.61875rem;
  border-radius: 100%;
  position: relative;
  border: 1.5px solid #aaa;
}
.checkbox label,
.radio label {
  min-height: 0;
  padding-left: 0;
}
.checbox label {
  display: block;
  width: 0.43125rem;
  height: 0.43125rem;
  border-radius: 100px;
  -webkit-transition: all .6s ease;
  -moz-transition: all .6s ease;
  -o-transition: all .6s ease;
  -ms-transition: all .6s ease;
  transition: all .6s ease;
  cursor: pointer;
  position: absolute;
  top: 0.09375rem;
  left: 0.09375rem;
  z-index: 1;
  background: #fff;
}
.checbox input[type=radio]:checked + label {
  background: #333;
}
.checbox input[type=checkbox]:checked + label {
  background: #3abeea;
}
.checbox input {
  opacity: 0;
}
